{
  "name": "next-starter-lite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|js|css|json)\" --write --list-different --config prettier.config.js",
    "type-check": "tsc",
    "start:test": "start-server-and-test 'yarn start' http://localhost:3000",
    "test": "yarn start:test 'cypress open'",
    "test:headless": "yarn start:test 'cypress run'"
  },
  "dependencies": {
    "@trpc/client": "^9.17.0",
    "@trpc/next": "^9.17.0",
    "@trpc/react": "^9.17.0",
    "@trpc/server": "^9.17.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-query": "^3.34.8",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@types/react": "latest",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.0",
    "cypress": "^9.2.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.6",
    "typescript": "^4.5.4"
  }
}
