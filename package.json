{
  "name": "next-starter-lite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|js|css|json)\" --write --list-different --config prettier.config.js",
    "type-check": "tsc",
    "start:test": "start-server-and-test 'yarn start' http://localhost:3000",
    "test": "yarn start:test 'cypress open'",
    "test:headless": "yarn start:test 'cypress run'"
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@types/react": "18.0.9",
    "@typescript-eslint/parser": "^5.25.0",
    "autoprefixer": "^10.4.7",
    "cypress": "^9.6.1",
    "eslint": "^8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  }
}
